  "license": "GPL v3",
  "scripts": {
    "robots": "cp src/components/SEO/robots.txt public/",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
